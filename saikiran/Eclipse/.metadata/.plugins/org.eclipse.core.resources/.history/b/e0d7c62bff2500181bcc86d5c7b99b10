public boolean panelService(Panel panel) throws SQLException {
	boolean success = false;
	try {
		panelDaoImpl.addPanel(panel);
		success = true;
	} catch(Exception e) {
		success = false;
		e.printStackTrace();
	}
	
	return success;
}

}
