package com.prokarma.sourcerer.dto;

public class PersonalTrait {
	private int traitId;
	private String personalTraitName;

	public int getTraitId() {
		return traitId;
	}

	public void setTraitId(int traitId) {
		this.traitId = traitId;
	}

	public String getPersonalTrait() {
		return personalTraitName;
	}

	public void setPersonalTrait(String personalTrait) {
		this.personalTraitName = personalTrait;
	}

}
