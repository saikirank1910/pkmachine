package com.sai.bestCodingPractices.two;

import java.util.Map;
import java.util.TreeMap;

public class CadApp {
	void drawShapes(Shape shape) {
	    Map<Integer, Shape> shapeMap= new TreeMap<Integer,Shape>();
		Shape shape = shapeMap.get(shape);
        shape.getType();
        shape.setShapeType();
        shape.getPoint();
        shape.setPoint();
	}
}
