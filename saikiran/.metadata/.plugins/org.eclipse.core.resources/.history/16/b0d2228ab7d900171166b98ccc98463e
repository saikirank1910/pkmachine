package com.sai.bestCodingPractices.six;

public class FoodDB{
	//findallfoodswhosenamescontainboththekeywords.returns
	//aniteratoronthesefoods.
	public Iterator (String cName,String name){
	//itcontainsallthefoodstobereturned.
	TreeMapfoodTree=newTreeMap();
	IteratorfoodList;
	Foodfood;
	foodList=getAllRecords();
	while(foodList.hasNext()){
	food=(Food)foodList.next();
	//doitsnamescontainbothkeywords?
	if((name==null||//nullor""meansnokeyisspecified
	name.equals("")||
	food.getName().indexOf(name)!=-1)){
	foodTree.put(food.getFoodKey(),food);
	}
	}
	returnfoodTree.values().iterator();
	}
	privateIteratorgetAllRecords(){
	//TODOAuto-generatedmethodstub
	returnnull;
	}
	}

