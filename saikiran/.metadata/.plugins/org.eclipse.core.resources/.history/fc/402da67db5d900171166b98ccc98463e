package com.sai.bestCodingPractices.three;

import java.util.Map;
import java.util.Scanner;

public class Currency {
	static Map<Integer, CurrencyFormat> currencyMap;
	public static void main(String[] args) {
		   Scanner read = new Scanner(System.in);
		   int n = read.nextInt();
		   Chinesse abc= new Chinesse();
		   
		   currencyMap.put(1,n);
		   printMoney(n);
	}
	 public static void printMoney(int money) {
	        CurrencyFormat print = currencyMap.get(money);
	        print.translateCurrency();
	    }
}
