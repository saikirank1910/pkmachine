	public static void main(String[] args) throws IOException {
		List<ThreadDemo> inst = new ArrayList<ThreadDemo>();
//		FileWriter fileWritter = new FileWriter("D:\\saikiran\\newEclipse\\ThreadsAssignment\\input.txt");
